
.title2{
  text-align: center;
  padding: 40rpx;
}
.text2{
  color:black;
}
.item{
  display: flex;
  justify-content:center;
  border-bottom: 6rpx solid #dedede;
  background: #fff;
  padding: 0 32rpx;
  flex-direction: column;
border-radius:5%;
border-top: 6rpx solid #dedede;
margin-top:30rpx;

}
.swiper {
  width: 100%;
}
.container {
  height:40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
  box-sizing: border-box;
} 
.anit{
  width: 100%;
  height: 70rpx;
  background: red;
  position: absolute;
  color: white;
  font-size: 30rpx;
  line-height: 70rpx;
  top: -70rpx;
  text-align: center;
}
.show{
  top: -70rpx;
  animation: show 10s;
  animation-timing-function:ease;
}
.img{width:50rpx;
height:50rpx;
float: right;
}
@keyframes show
{
  0% {top:-70rpx;}
  10% {top:0rpx;}
  80% {top:0rpx;}
  100% {top:-70rpx;}
}

